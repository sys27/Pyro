{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=pyro.db"
  },
  "Git": {
    "BasePath": ".",
    "UseNativeGitBackend": true
  },
  "Service": {
    "PublicUrl": "http://localhost"
  },
  "EmailService": {
    "Provider": "Console",
    "Host": "",
    "Domain": "localhost.local",
    "Login": "",
    "Password": ""
  },
  "Workers": {
    "OutboxMessageProcessing": {
      "Interval": "00:00:03",
      "BatchSize": 10
    },
    "RemoveExpiredOneTimePasswords": {
      "Interval": "00:30:00"
    }
  }
}